    "@sendgrid/mail": "^8.1.5",
    "@supabase/supabase-js": "^2.49.4",
    "@types/cheerio": "^0.22.35",
    "@types/pg": "^8.12.0",
    "@types/react-calendar": "^3.9.0",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "file-saver": "^2.0.5",
    "formidable": "^3.5.2",
    "framer-motion": "^12.6.3",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "next": "15.2.4",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.0",
    "papaparse": "^5.5.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.4.120",
    "pg": "^8.15.6",
    "playwright": "^1.52.0",
    "prisma": "^6.5.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-calendar": "^5.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
